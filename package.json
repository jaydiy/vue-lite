{
  "name": "vue-lite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "prepare": "husky install",
    "slint": "stylelint  \"src/**/*.{vue,scss,css,sass,less}\"",
    "slint:fix": "stylelint  \"src/**/*.{vue,scss,css,sass,less}\" --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "eslint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "svgo": "svgo -f src/icons/svg --config=src/utils/svgo.config.js"
  },
  "dependencies": {
    "@unocss/reset": "^0.55.2",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/tsconfig": "^0.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.1.0",
    "svgo": "^3.0.2",
    "typescript": "~5.1.6",
    "unocss": "^0.55.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
